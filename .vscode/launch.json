    "version": "0.2.0",
    "configurations": [
    {
        "name": "Attach",
        "port": 9229,
        "request": "attach",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "pwa-node"
    },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/InputTests/bin/Debug/netcoreapp3.1/InputTests.dll",
            "args": [],
            "cwd": "${workspaceFolder}/InputTests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (Player)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/PlayerCharacter/bin/Debug/netcoreapp3.1/PlayerCharacter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PlayerCharacter",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (INputs)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/InputTests/bin/Debug/netcoreapp3.1/InputTEsts.dll",
            "args": [],
            "cwd": "${workspaceFolder}/InputTests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Launch (Basic jeep)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BasicJeep/bin/Debug/netcoreapp3.1/BasicJeep.dll",
            "args": [],
            "cwd": "${workspaceFolder}/BasicJeep",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}